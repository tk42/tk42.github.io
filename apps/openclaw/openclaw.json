{
  "agents": {
    "defaults": {
      "model": {
        "primary": "openai/gpt-5.2"
      },
      "workspace": "/data"
    }
  },
  "tools": {
    "profile": "coding",
    "allow": [
      "group:fs",
      "group:runtime",
      "web_fetch",
      "group:memory",
      "image",
      "group:sessions"
    ],
    "deny": [
      "nodes",
      "canvas"
    ],
    "web": {
      "search": {
        "enabled": false
      },
      "fetch": {
        "enabled": true
      }
    },
    "exec": {
      "backgroundMs": 10000,
      "timeoutSec": 1800
    }
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  },
  "channels": {
    "mattermost": {
      "enabled": true,
      "botToken": "${MATTERMOST_BOT_TOKEN}",
      "baseUrl": "${MATTERMOST_URL}",
      "chatmode": "onmessage",
      "dmPolicy": "open",
      "allowFrom": [
        "*"
      ],
      "groupPolicy": "open",
      "requireMention": false,
      "replyToMode": "all",
      "groups": {
        "*": {
          "requireMention": false
        }
      }
    }
  },
  "gateway": {
    "mode": "local",
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  },
  "skills": {
    "entries": {
      "github": {
        "enabled": true
      },
      "summarize": {
        "enabled": true
      },
      "session-logs": {
        "enabled": true
      },
      "clawhub": {
        "enabled": true
      },
      "healthcheck": {
        "enabled": true
      },
      "url-reader": {
        "enabled": true
      },
      "knowledge-search": {
        "enabled": true
      }
    }
  },
  "plugins": {
    "entries": {
      "mattermost": {
        "enabled": true
      }
    }
  },
  "meta": {
    "lastTouchedVersion": "2026.2.13",
    "lastTouchedAt": "2026-02-14T08:39:23.199Z"
  }
}
